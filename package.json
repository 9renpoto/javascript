{
  "name": "@9renpoto/ts",
  "description": "study frontend",
  "version": "0.0.1",
  "author": "9renpoto",
  "devDependencies": {
    "@9renpoto/eslint-config": "^2.4.0",
    "@9renpoto/tslint-config": "^3.4.0",
    "codecov": "^3.0.0",
    "eslint": "^4.17.0",
    "fixpack": "^2.3.1",
    "husky": "^0.14.3",
    "lerna": "^2.8.0",
    "lint-staged": "^6.1.0",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.0",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^12.0.0"
  },
  "keywords": [
    "TypeScript",
    "codecov",
    "example"
