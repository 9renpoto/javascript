{
  "name": "@9renpoto/ts",
  "description": "study frontend",
  "version": "0.0.1",
  "author": "9renpoto",
  "devDependencies": {
    "@9renpoto/eslint-config": "4.6.0",
    "@9renpoto/tslint-config": "4.6.0",
    "codecov": "3.1.0",
    "eslint": "5.6.0",
    "fixpack": "2.3.1",
    "husky": "1.0.1",
    "lerna": "3.4.0",
    "lint-staged": "7.3.0",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.1",
    "typescript-eslint-parser": "19.0.1"
  },
  "keywords": [
    "TypeScript",
    "codecov",
    "example"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/javascript.git"
  },
  "scripts": {
    "build": "lerna run build",
    "coverage": "lerna run coverage",
    "lint": "lerna run lint",
    "precommit": "lint-staged",
    "test": "lerna run test"
  },
  "workspaces": [
    "packages/*"
  ]
}
