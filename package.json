{
  "name": "@9renpoto/ts",
  "description": "study frontend",
  "version": "0.0.1",
  "author": "9renpoto",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/javascript.git"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "cover:html": "cd coverage && remap-istanbul -i coverage.json -o html-report -t html",
    "cover": "npm-run-all -p cover:*",
    "cover:json": "cd coverage && cat coverage.json | remap-istanbul -o coverage-remapped.json",
    "codecov": "npm run cover && codecov -f coverage/coverage-remapped.json",
    "lint:ts": "tslint \"src/*.ts\" \"test/*.ts\"",
    "lint": "npm-run-all -p -l lint:*",
    "posttest": "npm run cover",
    "test:lint": "npm run lint",
    "test:karma": "karma start --singleRun true",
    "test": "npm-run-all -p test:*"
  },
  "directories": {
    "test": "test"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@9renpoto/tslint-config": "^1.0.0",
    "@types/mocha": "^2.2.29",
    "@types/power-assert": "0.0.27",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.0",
    "npm-run-all": "^2.3.0",
    "power-assert": "^1.4.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.4",
    "tslint": "^3.14.0",
    "typescript": "^2.0.0",
    "webpack": "^1.13.2"
  },
  "keywords": [
    "codecov",
    "example",
    "typescript"
  ]
}
