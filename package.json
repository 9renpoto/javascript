{
  "name": "@9renpoto/ts",
  "description": "study frontend",
  "version": "0.0.1",
  "author": "9renpoto",
  "devDependencies": {
    "@9renpoto/eslint-config": "4.4.0",
    "@9renpoto/tslint-config": "4.0.0",
    "codecov": "3.0.2",
    "eslint": "4.19.1",
    "fixpack": "2.3.1",
    "husky": "0.14.3",
    "lerna": "2.11.0",
    "lint-staged": "7.1.3",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "ts-node": "6.1.0",
    "tslint": "5.10.0",
    "typescript": "2.9.1",
    "typescript-eslint-parser": "15.0.0"
  },
  "keywords": [
    "TypeScript",
    "codecov",
    "example"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/javascript.git"
  },
  "scripts": {
    "allow-failsures": "run-p allow-failed:*",
    "build": "lerna run build",
    "coverage": "lerna run coverage",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "precommit": "lint-staged",
    "test": "lerna run test"
  }
}
