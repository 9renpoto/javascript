      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/javascript.git"
  },
  "scripts": {
    "allow-failsures": "run-p allow-failed:*",
    "build": "lerna run build",
    "coverage": "lerna run coverage",
