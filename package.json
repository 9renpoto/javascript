{
  "name": "@9renpoto/ts",
  "description": "study frontend",
  "version": "0.0.1",
  "author": "9renpoto",
  "devDependencies": {
    "@9renpoto/eslint-config-typescript": "5.2.0",
    "@types/jest": "24.0.18",
    "codecov": "3.6.1",
    "eslint": "6.5.1",
    "eslint-plugin-jest": "22.19.0",
    "husky": "3.0.8",
    "lerna": "3.17.0",
    "lint-staged": "9.4.2",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "3.0.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.4"
  },
  "keywords": [
    "TypeScript",
    "codecov",
    "example"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write",
      "git add"
    ],
    "*.{md,js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/ts.git"
  },
  "scripts": {
    "build": "lerna run build --parallel",
    "coverage": "lerna run coverage",
    "lint": "lerna run lint --parallel",
    "postinstall": "lerna bootstrap",
    "test": "lerna run test --parallel"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
