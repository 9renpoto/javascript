{
  "compilerOptions": {
    "listFiles": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "noImplicitAny": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "exclude": [
    "node_modules"
  ]
}
